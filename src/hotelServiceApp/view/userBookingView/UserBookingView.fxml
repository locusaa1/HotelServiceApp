      <ListView fx:id="bookingList" layoutX="64.0" layoutY="213.0" prefHeight="248.0" prefWidth="230.0" />
      <Button fx:id="deleteBookingButton" layoutX="219.0" layoutY="499.0" mnemonicParsing="false" onAction="#deleteBooking" text="Delete" />
