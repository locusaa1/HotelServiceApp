                </AnchorPane>
                <Label layoutX="135.0" layoutY="49.0" text="Modify the fields that you want"/>
                <Label layoutX="65.0" layoutY="358.0" text="Password:">
                    <font>
                        <Font name="Bodoni MT" size="14.0"/>
                    </font>
                </Label>
                <PasswordField fx:id="passwordTextField" layoutX="144.0" layoutY="354.0" promptText="password"/>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
