                    <font>
                        <Font name="Bodoni MT" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="addressLabel" layoutX="81.0" layoutY="238.0" prefHeight="17.0" prefWidth="59.0"
                       text="Address:">
                    <font>
                        <Font name="Bodoni MT" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="countryLabel" layoutX="81.0" layoutY="198.0" prefHeight="17.0" prefWidth="59.0"
                       text="Country:">
                    <font>
                        <Font name="Bodoni MT" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="dniLabel" layoutX="102.0" layoutY="158.0" prefHeight="17.0" prefWidth="34.0" text="Dni:">
                    <font>
                        <Font name="Bodoni MT" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="surnameLabel" layoutX="81.0" layoutY="118.0" prefHeight="17.0" prefWidth="59.0"
                       text="Surname:">
                    <font>
                        <Font name="Bodoni MT" size="14.0"/>
