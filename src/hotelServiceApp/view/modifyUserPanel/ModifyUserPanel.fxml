                </Label>
                <Label fx:id="nameLabel" layoutX="95.0" layoutY="78.0" prefHeight="17.0" prefWidth="48.0" text="Name:">
                    <font>
                        <Font name="Bodoni MT" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="usernameLabel" layoutX="63.0" layoutY="321.0" prefHeight="17.0" prefWidth="78.0"
                       text="Username:">
                    <font>
                        <Font name="Bodoni MT" size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="usernameTextField" layoutX="143.0" layoutY="317.0" promptText="username"
                           AnchorPane.topAnchor="317.0"/>
                <TextField fx:id="phoneTextField" layoutX="143.0" layoutY="274.0" promptText="phone"
                           AnchorPane.topAnchor="274.0"/>
                <TextField fx:id="addressTextField" layoutX="143.0" layoutY="234.0" promptText="address"/>
                <TextField fx:id="countryTextField" layoutX="143.0" layoutY="194.0" promptText="country"/>
