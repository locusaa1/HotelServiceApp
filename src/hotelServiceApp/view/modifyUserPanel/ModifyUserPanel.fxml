                       text="Passenger information">
                    <font>
                        <Font name="Bodoni MT Black" size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="dniTextFIeld" layoutX="143.0" layoutY="154.0" promptText="dni"/>
                <TextField fx:id="surnameTextField" layoutX="143.0" layoutY="114.0" promptText="surname"/>
                <TextField fx:id="nameTextField" layoutX="143.0" layoutY="74.0" promptText="name"/>
                <AnchorPane layoutX="366.0" layoutY="260.0" prefHeight="79.0" prefWidth="202.0"
                            style="-fx-background-color: #FFA15B;">
                    <children>
                        <Button fx:id="confirmButton" layoutX="70.0" layoutY="42.0" mnemonicParsing="false"
                                onAction="#confirmChanges" text="Confirm">
